# Configuration example for GenOCR
# Copy this file to .env and adjust values as needed

# Maximum upload size in bytes (default: 25MB)
MAX_CONTENT_LENGTH=26214400

# Application origin for CORS (optional)
# Example: http://127.0.0.1:8000 or https://your-domain.com
APP_ORIGIN=

# Enable deep table extraction models (experimental)
# Set to true to enable placeholder hook for neural table models
USE_DEEP_TABLES=false

# Run pipeline synchronously (useful for testing)
# Set to true for inline processing, false for async (production)
SYNC_PIPELINE=false

# Server port (default: 7860 for Hugging Face Spaces)
PORT=7860

# Force HTTPS redirect (set to true in production)
FORCE_HTTPS=false
